"/":
GET: returns html

"/register":
POST: { username, password, firstName, lastName }

"/login":
POST: { username, password }

"/posts":
GET:
POST: { content }

"/posts/<id:int>":
PUT: { content },
DELETE

"/posts/<id:int>/comments":
GET:
POST: { content }

"/posts/<id:int>/comments/<id:int>":
PUT: { content },
DELETE

"/posts/<id:int>/comments/<id:int>/responses":
GET: 
POST: { content }

"/posts/<id:int/comments/<id:int>/responses/<id:int>":
PUT: { content }
DELETE


